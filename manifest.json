{
  "manifest_version": 2,
  "name": "copyy",
  "version": "2",
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["contnent.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "logo.png"
  },
  "permissions": ["activeTab", "storage"]
}